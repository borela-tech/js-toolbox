{
  "name": "@borela-tech/js-toolbox",
  "version": "1.0.0",
  "description": "CLI tool to simplify the development of JavaScript apps/libraries with little to no configuration.",
  "license": "Apache-2.0",
  "bin": {
    "bb": "build/main.js"
  },
  "scripts": {
    "clean": "rimraf build",
    "dev": "npm run clean && run-p dev:*",
    "dev:build": "babel src -d build --ignore \"**/__tests__\" --source-maps",
    "dev:banner-assets": "cpx \"src/banner/*.txt\" build/banner",
    "dev-w": "npm run clean && run-p dev-w:*",
    "dev-w:build": "babel src -d build --ignore \"**/__tests__\" --source-maps --watch",
    "dev-w:banner-assets": "cpx \"src/banner/*.txt\" build/banner --watch",
    "lint": "bb lint",
    "test": "bb test",
    "test-w": "bb test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+//github.com/borela-tech/js-toolbox.git"
  },
  "bugs": {
    "url": "//github.com/borela-tech/js-toolbox/issues"
  },
  "homepage": "//www.borela.tech/#/js-toolbox",
  "keywords": [
    "babel",
    "borela",
    "cli",
    "dev",
    "develop",
    "development",
    "flow",
    "react",
    "tool",
    "toolbox",
    "typescript",
    "webpack"
  ],
  "author": {
    "name": "Alexandre Borela",
    "email": "alexandre@borela.tech",
    "url": "//www.borela.tech"
  },
