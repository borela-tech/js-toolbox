{
  "name": "@borela-tech/js-toolbox",
  "version": "1.0.0",
  "description": "CLI tool to simplify the development of JavaScript apps/libraries with little to no configuration.",
  "license": "Apache-2.0",
  "bin": {
    "bb": "build/index.js"
  },
  "scripts": {
    "clean": "rimraf build",
    "dev": "npm run clean && run-p \"dev:*\"",
    "dev:assets": "cpx \"src/banner/*.txt\" \"build/banner\"",
    "dev:build": "babel src -d build --ignore \"**/__tests__\" --source-maps inline",
    "dev-w": "npm run clean && run-p dev-w:*",
    "dev-w:assets": "cpx \"src/banner/*.txt\" \"build/banner\" --watch",
    "dev-w:build": "babel src -d build --ignore \"**/__tests__\" --source-maps inline --watch",
    "lint": "bb lint",
    "test": "bb test",
    "test-w": "bb test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+//github.com/borela-tech/js-toolbox.git"
  },
  "bugs": {
    "url": "//github.com/borela-tech/js-toolbox/issues"
  },
  "homepage": "//www.borela.tech/#/js-toolbox",
  "keywords": [
    "borela",
    "dev",
    "develop",
    "development",
    "tool",
    "toolbox"
  ],
  "author": {
    "name": "Alexandre Borela",
    "email": "alexandre@borela.tech",
    "url": "//www.borela.tech"
  },
  "dependencies": {
